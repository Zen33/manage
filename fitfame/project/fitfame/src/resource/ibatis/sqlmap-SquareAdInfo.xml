<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="SquareAdInfo">
	<typeAlias alias="SquareAdInfo" type="fitfame.po.SquareAdInfo" />

	<select id="getSquareAdInfoList" resultClass="SquareAdInfo"
		parameterClass="java.util.Map">
		select * from square_ad_info
	</select>
	
	<insert id="insertSquareAdInfo" parameterClass="SquareAdInfo">
		insert into square_ad_info values (#cid#, #url#)
	</insert>

	<delete id="deleteSquareAdInfo">
		delete from square_ad_info where url = #value#
	</delete>
</sqlMap>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="CommonCalendar">
	<typeAlias alias="CommonCalendar" type="fitfame.po.CommonCalendar" />

	<select id="getMonthCalendar" resultClass="CoachAdInfo">
		<![CDATA[ 
		select * from common_calendar where cdate > #value# and cdate-32 < #value#
		 ]]> 
	</select>
</sqlMap>
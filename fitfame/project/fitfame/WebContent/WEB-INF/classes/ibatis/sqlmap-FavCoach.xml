<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="FavCoach">
	<typeAlias alias="FavCoach" type="fitfame.po.FavCoach" />

	<select id="getFavCoach" parameterClass="FavCoach" resultClass="FavCoach">
		select * from fav_coach where cid = #cid# and uid = #uid#
	</select>

	<insert id="insertFavCoach" parameterClass="FavCoach">
		insert into fav_coach values (#cid#, #uid#)
	</insert>
</sqlMap>